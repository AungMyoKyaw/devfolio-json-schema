{
  "name": "devfolio-json-schema",
  "version": "1.0.0-beta.0",
  "description": "A comprehensive TypeScript-first JSON schema for professional developer portfolios with Zod validation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node -e \"console.log('Testing schema...'); require('./dist/test.js')\"",
    "validate": "node -e \"require('./dist/examples/validate-sample.js')\"",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "json-schema",
    "portfolio",
    "resume",
    "developer",
    "zod",
    "typescript",
    "validation",
    "professional"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AungMyoKyaw/devfolio-json-schema.git"
  },
  "author": "Aung Myo Kyaw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AungMyoKyaw/devfolio-json-schema/issues"
  },
  "homepage": "https://github.com/AungMyoKyaw/devfolio-json-schema#readme",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.5.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
